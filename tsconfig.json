{
    "compilerOptions": {
      "target": "ES2019",
      "module": "ESNext",
      "moduleResolution": "Node",
      "lib": [
        "ESNext",
        "ESNext.AsyncIterable",
        "DOM"
      ],
      "noImplicitAny": false, // default true
      "resolveJsonModule": true,
      "esModuleInterop": true,
      "allowJs": true,
      "sourceMap": true,
      "strict": true,
      "noEmit": true,
      "experimentalDecorators": true,
      "baseUrl": ".",
      "paths": {
        "~/*": [
          "./*"
        ],
        "@/*": [
          "./*"
        ]
      },
      "types": [
        "node",
        "@nuxt/types",
        "@nuxtjs/axios",
        "@types/node",
        "webdriverio/async",
        "@wdio/mocha-framework",
        "expect-webdriverio",
        "@nuxtjs/i18n",
        "vue-the-mask.d.ts",
        "@wdio/devtools-service"
      ]
    },
    "exclude": [
      "node_modules",
      ".nuxt",
      "dist",
      "public"
    ]
  }
  